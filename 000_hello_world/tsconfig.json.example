{
    "compilerOptions": {
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "allowJs": true,
        "baseUrl": ".",
        "moduleResolution": "node",
        "outDir": "./dist",
        "module": "es6",
        "target": "es5",
    },
    "include": [
        "*.ts"
    ]
}
